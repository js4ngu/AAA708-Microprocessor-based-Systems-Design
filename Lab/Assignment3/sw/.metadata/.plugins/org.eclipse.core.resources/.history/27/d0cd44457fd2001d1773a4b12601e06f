#include "sleep.h"
#include "lab0.h"

#define ON1 0x000000000
#define ON2 0x010000000
#define ON3 0x000800000
#define ON4 0x000400000
#define ON5 0x008200000

int aes_main() {
    uint32_t r3_value;
    asm volatile ("mov %[r3_value], r3" : [r3_value] "=r" (r3_value));

	if((r3_value == ON1)||(r3_value == ON2)||(r3_value == ON3)||(r3_value == ON4)||(r3_value == ON5)) usleep(100000);
	else usleep(500000);
	return 0;
}
